{
    "name"              : "simtabi/pheg",
    "license"           : "MIT",
    "description"       : "A collection of handy and useful utility library for the everyday PHP developer",
    "type"              : "package",
    "keywords": [
        "helpers",
        "functions"
    ],
    "authors": [
        {
            "name": "Imani Manyara",
            "email": "hello@imanimanyara.com",
            "homepage": "https://www.imanimanyara.com",
            "role": "Developer"
        }
