{
    "name"              : "simtabi/pheg",
    "license"           : "MIT",
    "description"       : "A collection of handy and useful utility library for the everyday PHP developer",
    "type"              : "package",
    "keywords": [
        "helpers",
        "functions"
    ],
    "authors": [
        {
            "name": "Imani Manyara",
            "email": "hello@imanimanyara.com",
            "homepage": "https://www.imanimanyara.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.2",
        "ext-gd": "*",
        "ext-dom": "*",
        "ext-libxml": "*",
        "ext-json": "*",
        "ext-exif": "*",
        "ext-ctype": "*",
        "ext-intl": "*",
        "ext-bcmath": "*",
        "ext-mbstring": "*",
        "ext-fileinfo": "*",
        "symfony/filesystem": "*",
        "guzzlehttp/guzzle": "@dev",
        "myclabs/deep-copy": "@dev",
        "eclipxe/xmlschemavalidator": "@dev",
        "league/color-extractor": "@dev",
        "annexare/countries-list": "@dev",
        "adbario/php-dot-notation": "@dev",
        "ramsey/uuid": "@dev",
        "spomky-labs/otphp": "@dev",
        "nesbot/carbon": "@dev",
        "cocur/slugify": "@dev",
        "vincoweb/easy-php-file-info": "@dev",
        "respect/validation": "@dev",
        "bjeavons/zxcvbn-php": "@dev",
        "giggsey/libphonenumber-for-php": "@dev",
        "irfa/php-sn-generator": "@dev",
        "html2text/html2text": "@dev",
        "phlak/strgen": "@dev",
        "gpslab/base64uid": "@dev",
        "ezyang/htmlpurifier": "@dev",
        "fightbulc/moment": "@dev",
        "maxakawizard/json-collection-parser": "@dev",
        "simtabi/json-objects": "@dev",
        "symfony/uid": "@dev",
        "jimmiw/php-time-ago": "@dev",
        "sokil/php-isocodes": "@dev",
        "masterminds/html5": "@dev",
        "spatie/url": "@dev",
        "justinrainbow/json-schema": "@dev",
        "seld/jsonlint": "@dev",
        "coduo/php-humanizer": "@dev",
        "rinvex/countries": "@dev",
        "soundasleep/html2text": "@dev"
    },
    "require-dev": {
        "ext-apache": "*",
        "symfony/translation": "@dev",
        "symfony/yaml": "@dev",
        "aeon-php/calendar": "@dev",
        "thunderer/shortcode": "@dev",
        "phpunit/phpunit": "@dev",
        "php-coveralls/php-coveralls": "@dev",
        "vimeo/psalm": "@dev"
    },

    "suggest": {
        "ext-bcmath": "Compare time units with high precision",
        "voku/stringy": "A PHP string manipulation library with multibyte support."
    },
    "autoload": {
        "psr-4": {
            "Simtabi\\Pheg\\": "src/"
        },
        "files": [
            "helpers/classes.php",
            "helpers/constants.php",
            "helpers/form_helpers.php"
        ]
    },
    "extra": {
    }
}
